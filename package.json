{
  "name": "hype-knowledge",
  "version": "0.1.0",
  "description": "High-performance hybrid RAG backend with vector search and graph traversal",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testMatch=\"**/tests/unit/**/*.test.ts\"",
    "test:integration": "jest --testMatch=\"**/tests/integration/**/*.test.ts\""
  },
  "dependencies": {
    "@qdrant/js-client-rest": "^1.6.0",
    "@langchain/community": "^0.0.17",
    "@langchain/core": "^0.1.12",
    "@langchain/openai": "^0.0.10",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.0.164",
    "multer": "^1.4.5-lts.1",
    "neo4j-driver": "^5.12.0",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdf-parse": "^1.1.1",
    "reflect-metadata": "^0.1.13",
    "redis": "^4.6.10",
    "unstructured-client": "^0.7.13",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.8.9",
    "@types/passport": "^1.0.14",
    "@types/passport-google-oauth20": "^2.0.13",
    "@types/passport-jwt": "^3.0.12",
    "@types/passport-local": "^1.0.37",
    "@types/supertest": "^2.0.15",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "rag",
    "ai",
    "knowledge",
    "vector-search",
    "graph-traversal",
    "neo4j",
    "qdrant",
    "langchain"
  ],
  "author": "",
